<?xml version="1.0" encoding="UTF-8"?>
<mule xmlns="http://www.mulesource.org/schema/mule/core/2.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:jms="http://www.mulesource.org/schema/mule/jms/2.1" xmlns:test="http://www.mulesource.org/schema/mule/test/2.1"
	xsi:schemaLocation="
			http://www.mulesource.org/schema/mule/core/2.1 http://www.mulesource.org/schema/mule/core/2.1/mule.xsd
			http://www.mulesource.org/schema/mule/jms/2.1 http://www.mulesource.org/schema/mule/jms/2.1/mule-jms.xsd
			http://www.mulesource.org/schema/mule/test/2.1 http://www.mulesource.org/schema/mule/test/2.1/mule-test.xsd">

	<jms:activemq-connector name="JmsConnector" brokerURL="vm://crepol-broker?create=false" />

	<model name="TestAdaptiveRetryPolicyTemplate">

		<service name="TestService">
			<inbound>
				<jms:inbound-endpoint queue="targetQueue" connector-ref="JmsConnector" />
			</inbound>
			<test:component enableMessageHistory="true" />
		</service>

	</model>

</mule>
